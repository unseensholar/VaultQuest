    "builtin-modules": "3.3.0",
    "esbuild": "^0.25.0",
    "obsidian": "latest",
    "tslib": "2.4.0",
    "typescript": "4.7.4"
  },
  "dependencies": {
    "crypto-js": "^4.2.0"
  }
